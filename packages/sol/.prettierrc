{
    "plugins": ["prettier-plugin-solidity"],
    "arrowParens": "avoid",
    "printWidth": 120,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "tabWidth": 4,
    "useTabs": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "preserve"
}
